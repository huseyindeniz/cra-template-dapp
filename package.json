{
  "name": "@huseyindeniz/cra-template-dapp",
  "version": "1.0.0-beta",
  "description": "dApp CRA Template",
  "main": "template.json",
  "files": [
    "template",
    "template.json"
  ],
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huseyindeniz/cra-template-dapp.git",
    "directory": "/"
  },
  "keywords": [
    "cra-template",
    "cra-template-typescript",
    "dapp",
    "web3",
    "ethers"
  ],
  "author": "HÃ¼seyin Deniz KIVRAK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huseyindeniz/cra-template-dapp/issues"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@metamask/jazzicon": "^2.0.0",
    "@react-icons/all-files": "^4.1.0",
    "@reduxjs/toolkit": "^1.9.1",
    "chakra-ui-steps": "^1.8.0",
    "ethers": "^5.7.2",
    "framer-motion": "^6.5.1",
    "i18next": "^22.4.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "react": "^18.2.0",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.1",
    "redux-saga": "^1.2.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@chakra-ui/icons": "^2.0.14",
    "@snek-at/storybook-addon-chakra-ui": "^1.1.0",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/node-logger": "^6.5.15",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.8",
    "@types/node": "^12.20.55",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.10",
    "@types/redux-mock-store": "^1.0.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "i18next-parser": "^7.1.0",
    "postcss": "8.4.19",
    "prop-types": "^15.8.1",
    "redux-mock-store": "^1.5.4",
    "redux-saga-test-plan": "^4.0.6",
    "source-map-explorer": "^2.5.3",
    "storybook-addon-react-router-v6": "^0.2.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0"
  },
  "resolutions": {
    "postcss": "8.4.19"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx ./src",
    "test": "react-scripts test --verbose --no-cache --watchAll",
    "coverage": "react-scripts test --coverage --no-cache --watchAll",
    "extract": "i18next 'src/**/*.{ts,tsx}'",
    "build": "react-scripts build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "clean-files": "rm -rf ./template/public ./template/src",
    "copy-files": "cp -a ./src/. template/src && cp -a ./public/. template/public",
    "prepublishOnly": "npm run clean-files && npm run copy-files"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/*.d.{ts}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/template/"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 90,
        "statements": 90
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
